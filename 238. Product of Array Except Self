https://leetcode.com/problems/product-of-array-except-self/description/

class Solution {
    public int[] productExceptSelf(int[] nums) {
        int p=1;
        int[] res=new int[nums.length];
        for(int i=0;i<nums.length;i++)
        {
            p*=nums[i];
        }
        for(int i=0;i<nums.length;i++)
        {
            if(nums[i]==0)
            {
                int val=1;
                for(int j=0;j<nums.length;j++)
                {
                    if(j==i)
                    {
                        continue;
                    }
                    val*=nums[j];
                }
                res[i]=val;
            }
            else
            {
            res[i]=(int)p/nums[i];
            }
        }
        return res;
        
    }
}
